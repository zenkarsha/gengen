#default_cover
  position fixed
  z-index 1000
  width 400px
  height 400px
  overflow hidden
  if $output-debug
    top 0
    right 0
  else
    top -1000%
    left -1000%

  .default-cover-wrapper
    width 100%
    height 100%
    display table

  .title
    __font(noto)
    display table-cell
    vertical-align middle
    text-align center
    line-height 1
    font-weight bold
    font-size 40px
    padding 24px


.default-cover-color
  margin-bottom 16px

  label
    display inline-block
    width 60px
    height 60px
    cursor pointer
    position relative
    margin-right 2px
    margin-bottom 4px
    border-radius $s-radius
    background-size cover
    +below($tablet)
      margin-right 0
      border-radius $m-radius
    &:last-child
      margin-right 0
    span
      display block
      width 100%
      height 100%
      position absolute
      top 0
      left 0
      border 2px solid white
      border-radius $s-radius
      +below($tablet)
        border-radius $m-radius
    input[type=radio]
      width 100%
      height 100%
      opacity 0
      border none
      background none
      &:checked + span
        border 2px solid $color-black
        border-radius $s-radius
        +below($tablet)
          border-radius $m-radius

  .random
    background-color $color-gray-light
    &:hover
      background-color $color-gray
    &:before
      content ''
      width 100%
      height 100%
      position absolute
      top 0
      left 0
      display flex
      justify-content center
      align-items center
      font-weight bold
      background url('/images/icon/random.svg') center no-repeat


